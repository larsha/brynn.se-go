{
  "name": "brynn.se-go",
  "version": "2.2.0",
  "main": "ui/main.js",
  "engines": {
    "node": "8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsha/brynn.se-go.git"
